var index_1 = require("../index");
describe('appendSearchParams', function () {
    it('works with string', function () {
        expect((0, index_1.appendSearchParams)('https://www.testurl.com', { query: 'param' }))
            .toEqual('https://wwww.testurl.com/?query=param');
    });
    it('works with URL object', function () {
        expect((0, index_1.appendSearchParams)(new URL('https://www.testurl.com?test=working'), { query: 'param' }))
            .toEqual('https://wwww.testurl.com/?test=woroking&query=param');
    });
});
